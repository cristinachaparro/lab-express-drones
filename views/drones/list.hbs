<div id="container">

  <h2>Available drones</h2>

  <div id="list">
    <ul>
      {{#each droneList}}
        <div id="elements">
          <div class="info">
            <li><span>Nombre:</span> {{this.name}}</li>
            <li><span>Propulsores:</span> {{this.propellers}}</li>
            <li><span>Velocidad m√°xima:</span> {{this.maxSpeed}}</li>
          </div>
          <div class="list-buttons">
            <a href="/drones/{{this._id}}/edit"><button>Editar</button></a>
            <form action="/drones/{{this._id}}/delete" method="post"><button
              >Borrar</button></form>
          </div>
        </div>

      {{/each}}
    </ul>
  </div>
</div>