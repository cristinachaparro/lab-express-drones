<h2>Available drones</h2>

<div>
  <ul>
    {{#each droneList}}
    <li>
        <p> {{this.name}} </p>
        <p> {{this.propellers}} </p>
        <p> {{this.maxSpeed}} </p>
        <a href="/drones/{{this._id}}/edit"><button>Editar</button></a>
        <form action="/drones/{{this._id}}/delete" method="post"><button>Borrar</button></form>
    </li>
    {{/each}}
  </ul>

</div>